mutate(hobo_event_n = recession_n)%>%
mutate(hobo_event_n = case_when(str_detect(hobo_event_n, "E1") ~ "1",
str_detect(hobo_event_n, "E2") ~ "2",
str_detect(hobo_event_n, "E3") ~ "3",
str_detect(hobo_event_n, "E4") ~ "4",
