hobo_rec <- hobo_all %>%
mutate(.after = TFD_mm, #indicates where the new column is placed
dt2 = as.POSIXct(dt, format = "%m/%d/%Y %H:%M")) %>%
filter(minute(dt2) == 0) %>% #remove subhourly timestamps
